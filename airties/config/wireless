config wifi-device  radio0
	option type     mac80211
	option channel  11
	option macaddr	00:1c:a8:12:34:56
	option hwmode	11g

	# REMOVE THIS LINE TO ENABLE WIFI:
	option disabled 1

config wifi-iface
	option device   radio0
	option network  lan
	option mode     ap
	option ssid     OpenWrt
	option encryption none

